.boxes {
  text-align: center;
  position: relative;
  width: 100%;
  margin: -200px 0;
}
.boxes .box {
border:1px solid hsl(0, 0%, 40%);
  display: block;
  float: left;
  position: relative;
  width: 29%;
  height: 450px;
  text-align: center;
  border-collapse: collapse;
  text-decoration: none;
  color: black;
  background: #222;
  margin: 0 10px;
  -webkit-transition: all 1s;
  transition: all 1s;
}
.boxes .box h3 {
  transition: all 1s;
  padding-top: 50px;
  padding-bottom: 60px;
  margin: 0;
  color:hsl(0, 0%, 96%);
  font-size: 2em;
  text-transform: uppercase;
}
.boxes .box i {
  transition: all 1s;
  font-size: 4em;
}
.boxes .box p {
  color:hsl(0, 0%, 96%);
  transition: all 1s;
  margin-top: 70px;
}



.boxes .box:before {
  transition: all 1s;
  content: "";
  width: 100%;
  height: 50%;
  left: 50%;
  margin-left: -50%;
  top: 25%;
  border-color: rgba(200, 200, 200, 0);
  border-style: solid;
  border-width: 0 1px;
}
.boxes .box:after {
  transition: all 1s;
<!--  position: absolute;-->
  content: "";
  width: 50%;
  height: 100%;
  left: 50%;
  margin-left: -25%;
  top: 0;
  border-color: white;
  border-style: solid;
  border-width: 1px 0;
}
.boxes .box:hover {
border:1px solid hsl(0, 0%, 90%);
  width:42%;
  height:500px;
  color: #4099ff;
}
.boxes .box:hover h3 {
  padding-top: 30px;
  padding-bottom: 30px;
}
.boxes .box:hover p {
  margin-top: 30px;
}
.boxes .box:hover:after {
  width: 402px;
  margin-left: -50%;
  border-color: rgba(90, 90, 90, 1);
}

